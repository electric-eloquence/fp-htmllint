{
  "name": "fp-htmllint",
  "version": "0.2.1",
  "description": "htmllint extension for Fepper",
  "main": "htmllint~extend.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint *.js && eslint test/*.js",
    "postinstall": "node install.js",
    "test": "nyc mocha"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electric-eloquence/fp-htmllint.git"
  },
  "keywords": [
    "atomic",
    "component",
    "design",
    "end",
    "fepper",
    "front",
    "front-end",
    "frontend",
    "htmllint",
    "lab",
    "library",
    "lint",
    "pattern",
    "prototype",
    "prototyper",
    "prototyping",
    "template"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/electric-eloquence/fp-htmllint/issues"
  },
  "homepage": "https://github.com/electric-eloquence/fp-htmllint#readme",
  "engines": {
    "node": ">=8.16.0"
  },
  "dependencies": {
    "fepper-utils": "*",
    "gulp-htmllint": "^0.0.16"
  },
  "devDependencies": {
    "chai": "4.x",
    "coveralls": "3.x",
    "eslint": "5.x",
    "fepper": "*",
    "glob": "^7.1.2",
    "gulp": "https://github.com/electric-eloquence/gulp/tarball/v3-lts",
    "husky": "1.x",
    "mocha": "6.x",
    "nyc": "14.x",
    "slash": "^2.0.0"
  }
}
